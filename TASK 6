CREATE TABLE Student (
  student_id INT PRIMARY KEY,
  name VARCHAR(50),
  age INT,
  department VARCHAR(30)
);

CREATE TABLE Course (
  course_id INT PRIMARY KEY,
  course_name VARCHAR(50),
  credits INT
);

CREATE TABLE Enrollment (
  enroll_id INT PRIMARY KEY,
  student_id INT,
  course_id INT,
  grade CHAR(2),
  FOREIGN KEY (student_id) REFERENCES Student(student_id),
  FOREIGN KEY (course_id) REFERENCES Course(course_id)
);
CREATE OR REPLACE PROCEDURE display_student_info IS
  CURSOR s_cur IS SELECT name, department FROM Student;
  s_row s_cur%ROWTYPE;
BEGIN
  OPEN s_cur;
  LOOP
    FETCH s_cur INTO s_row;
    EXIT WHEN s_cur%NOTFOUND;
    DBMS_OUTPUT.PUT_LINE('Name: ' || s_row.name || ' Dept: ' || s_row.department);
  END LOOP;
  CLOSE s_cur;
END;
